 document.addEventListener("DOMContentLoaded", function () {
    var bundleDivs = document.querySelectorAll('.custom-bundle-inner');
    var discountBadge = document.querySelector('.discount-badge');
    var firstBundleDiv = document.querySelector('.custom-bundle-inner');
    var compareAtPriceElement = firstBundleDiv.querySelector('.compare-at-price');
    var textBlackElement = firstBundleDiv.querySelector('.text-black');

    if (compareAtPriceElement && textBlackElement && discountBadge) {
      var compareAtPrice = parseFloat(compareAtPriceElement.textContent.replace('$', '').trim());
      var textBlackValue = parseFloat(textBlackElement.textContent.replace('$', '').trim());
      var defaultDiscountPercentage = ((compareAtPrice - textBlackValue) / compareAtPrice) * 100;
      discountBadge.innerText = defaultDiscountPercentage.toFixed(0) + '% off';

      bundleDivs.forEach(function (bundleDiv) {
        bundleDiv.addEventListener('click', function () {
          var clickedCompareAtPriceElement = bundleDiv.querySelector('.compare-at-price');
          var clickedTextBlackElement = bundleDiv.querySelector('.text-black');

          if (clickedCompareAtPriceElement && clickedTextBlackElement && discountBadge) {
            var clickedCompareAtPrice = parseFloat(clickedCompareAtPriceElement.textContent.replace('$', '').trim());
            var clickedTextBlackValue = parseFloat(clickedTextBlackElement.textContent.replace('$', '').trim());
            var clickedDiscountPercentage = ((clickedCompareAtPrice - clickedTextBlackValue) / clickedCompareAtPrice) * 100;
            discountBadge.innerText = clickedDiscountPercentage.toFixed(0) + '% off';
          }
        });
      });
    }
});


   function toggleCheckbox(label) {
      var checkbox = label.querySelector('input[type="checkbox"]');
      var spanElement = label.querySelector('span');
      var svgElement = label.querySelector('.custom-svg');
  
      // Toggle the checkbox state
      checkbox.checked = !checkbox.checked;
  
      // Update the text and SVG based on the checkbox state
      if (checkbox.checked) {
        spanElement.textContent = 'Added';
        svgElement.src = 'https://cdn.shopify.com/s/files/1/0764/0530/9760/files/check-red.svg?v=1701429571';
        label.style.backgroundColor = "#fff0"; // Optional: Change background color when checked
      } else {
        spanElement.textContent = 'Add';
        svgElement.src = 'https://cdn.shopify.com/s/files/1/0764/0530/9760/files/add.svg?v=1701384974';
        label.style.backgroundColor = "#fff0"; // Optional: Reset background color when unchecked
      }
    }
// Get all .upsell-text-wrap elements
      const noupsellTextWraps = document.querySelectorAll('.upcover .upsell-text-wrap');

      // Check if both .upsell-cont and .upsell-badge elements within each .upsell-text-wrap are empty
      noupsellTextWraps.forEach(upsellTextWrap => {
        const paragraphsCont = upsellTextWrap.querySelector('.upsell-cont p');
        const paragraphsBadge = upsellTextWrap.querySelector('.upsell-badge p');
        
        if (paragraphsCont.innerHTML.trim() === '' && paragraphsBadge.innerHTML.trim() === '') {
          upsellTextWrap.classList.add('empty');
        }
      });



