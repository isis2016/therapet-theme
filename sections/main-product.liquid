                    const formData = new FormData($(complementaryProduct).parents('.card').find('product-form form').get(0));
                    config.body = formData;
                    const a = await fetch('/cart/add', config).then(response => response.json())
                    .then(data => {
                      // Handle the response data
                      console.log('Product added to the cart:', data);
                    })
                    .catch(error => {
                      // Handle errors
                      console.error('Error adding product to the cart:', error);
                    });
                  }

                
