  <div style="    padding-top: 1rem;"
    id="MainProduct-{{ section.id }}"
    data-section="{{ section.id }}"
    class="px-4 py-10 lg:px-12 lg:py-20"
  >
    <div class="max-w-screen-xl mx-auto" id="order-now-section">
      <div class="grid grid-cols-1 md:grid-cols-2 gap-10">
        <div class="product-slider">
          <!-- slider -->
          <div id="my-keen-slider" class="keen-slider">
            {% if product.images.size > 0 %}
              {% for image in product.images %}
                <div class="keen-slider__slide">
                  <div class="aspect-square">
                    {{ image | image_url: width: 800 | image_tag: class: 'w-full' }}
                  </div>
                </div>
              {% endfor %}
            {% else %}
              <div>
                {{ 'image' | placeholder_svg_tag }}
              </div>
            {% endif %}
          </div>
          <!-- Navigation Markup -->
          {% if product.images.size > 1 %}
            <div class="flex justify-between pt-3">
              <div class="arrow flex items-center arrow--left rotate-180">
                {% render 'icon' with 'arrow-right' %}
              </div>
              <div class="flex gap-2 items-center justify-center">
                {% for image in product.images %}
                  <div class="dot block w-3 h-3 rounded-full bg-[#c5c5c5]"></div>
                {% endfor %}
              </div>
              <div class="arrow flex items-center arrow--right">
                {% render 'icon' with 'arrow-right' %}
              </div>
            </div>
          {% endif %}
        </div>

        <div class="product-info">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when 'title' %}
                <div class="flex items-start justify-between w-full max-w-[500px]">
                  <h2 class="text-[20px] leading-[24px] md:text-2xl font-extrabold md:leading-7 tracking-[0.6px] md:tracking-wide text-neutral-800 lg:text-3xl">
                    {{ product.title }}
                  </h2>
              {% if block.settings.title_label != blank %}
                  <span class="flex-shrink-0 tracking-[0.6px] px-[10px] py-[5px] md:px-[15px] md:py-[7px] bg-[#7A5EC5] rounded text-[14px] text-white leading-[17px]">
                    {{ block.settings.title_label }}
                  </span>
              {% endif %}
                </div>


