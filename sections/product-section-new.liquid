<script>
  document.addEventListener('DOMContentLoaded', () => {
    const minPeople = {{block.settings.min}};
    const maxPeople = {{block.settings.max}};
    const peopleElement = document.querySelector('.people-count');
    const updateInterval = 7000; // milliseconds

    function getRandomNumber(min, max) {
      return Math.floor(Math.random() * (max - min + 1)) + min;
    }

    function updatePeopleCount() {
      const peopleCount = getRandomNumber(minPeople, maxPeople);
      peopleElement.textContent = `${peopleCount} people`;
    }

    setInterval(updatePeopleCount, updateInterval);
  });
</script>

              
