                  <p>{{ section.settings.reviewText }}</p>
                </div>
              
              {% when 'price' %}
                <div class="mt-3 flex flex-col md:flex-row gap-1 sm:gap-2 md:gap-4 items-baseline">
                  {% if product.compare_at_price_max > product.price %}
                    <p>
                      <s class="text-neutral-400 text-[28px] leading-[38px] tracking-wider font-bold">
                        {{ product.compare_at_price_max | money_with_currency }}
                      </s>
                    </p>
                    <p class="text-[24px] leading-[32px]">{{ product.price | money_with_currency }}</p>
                  {% else %}
                    <p class="text-neutral-400 text-[28px] leading-[38px] tracking-wider font-bold">
                      {{ product.price | money_with_currency }}
                    </p>
                  {% endif %}
                </div>

              {% when 'alternate_price' %}
                <div class="mt-3">
                  {% if product.compare_at_price_max > product.price %}
                    {% assign discount = product.compare_at_price_max | minus: product.price %}
                    <p class="inline-block">
                      <s class="text-[28px] font-bold leading-[34px] tracking-wider text-neutral-400">
                        {{ product.compare_at_price_max | money_with_currency }}
                      </s>
                    </p>
                    <p class="inline-block text-[21px] font-bold leading-[25px] tracking-wider">
                      {{ product.price | money_with_currency }}
                    </p>
                    <span class="inline-block text-[20px] font-bold leading-[24px] tracking-wider text-[#39CD59]"
                      >(YOU SAVE {{ discount | money }})</span
                    >
                  {% else %}
                    <p>{{ product.price | money_with_currency }}</p>
                  {% endif %}
                </div>

              {% when 'review_bar' %}
                <div class="flex gap-1 items-center mt-3">
                  {% render 'icon' with 'icon-rating' %}
                  <p>{{ block.settings.review_bar_text }}</p>
                </div>

              {% when 'emi' %}
                <div id="emi-box" class="mt-3 max-w-[500px]">
                  <p class="text-[14px] leading-[27px] text-black tracking-wider font-semibold">
                    Choose a payment option:
                  </p>
                  <div class="grid grid-cols-2 rounded-lg border border-gray-200 mt-1">
                    <div data-payment-option="pay-now" class="md:px-4 md:py-3 p-2 cursor-pointer">
                      <div class="flex">
                        <div
                          data-option-radio
                          class="h-5 w-5 rounded-full mt-1 mr-2 border-[6px] border-[#7A5EC5] flex-shrink-0"
                        ></div>
                        <div class="w-full">
                          <p class="text-[15px] leading-[27px] text-black tracking-wider font-semibold">Pay in full</p>
                          <p
                            data-element-type="pay-full-price"
                            class="text-[15px] text-black tracking-wider font-semibold singlePaymentAll"
                          >
                            {{ product.price | money_without_trailing_zeros }}
                          </p>
                          <div class="mt-1 pt-1 border-t border-gray-200">
                            <p class=" text-[11px] leading-[15px] tracking-wider">One-time payment with</p>
                            <ul>
                              <li class="inline-flex">
                                {% render 'icon' with 'processor-amex' %}
                              </li>
                              <li class="inline-flex">
                                {% render 'icon' with 'processor-discover' %}
                              </li>
                              <li class="inline-flex">
                                {% render 'icon' with 'processor-mastercard' %}
                              </li>
                              <li class="inline-flex">
                                {% render 'icon' with 'processor-visa' %}
                              </li>
                              <li class="inline-flex">
                                {% render 'icon' with 'processor-paypal' %}
                              </li>
                            </ul>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div
                      data-payment-option="pay-later"
                      class="md:px-4 md:py-3 p-2 border-l border-gray-200 cursor-pointer"
                    >
                      <div class="flex">
                        <div
                          data-option-radio
                          class="h-5 w-5 rounded-full mt-1 mr-2 border border-[#7A5EC5] flex-shrink-0"
                        ></div>
                        <div class="w-full">
                          <p class="text-[15px] leading-[27px] text-black tracking-wider font-semibold">
                            Pay today only
                          </p>
                          <p
                            data-element-type="pay-part-price"
                            class="text-[15px] text-black tracking-wider font-semibold dividePaymentSplit"
                          >
                            {{ product.price | divided_by: 4 | money_without_trailing_zeros }}
                          </p>
                          <div class="mt-1 pt-1 border-t border-gray-200">
                            <p class=" text-[11px] leading-[15px] tracking-wider">
                              Monthly interest-free payments with
                            </p>
                            <ul>
                              <li class="inline-flex">
                                {% render 'icon' with 'processor-shoppay' %}
                              </li>

                              {% comment %}
                              <li class="inline-flex">
                                {% render 'icon' with 'processor-klarana' %}
                              </li>
                              <li class="inline-flex">
                                {% render 'icon' with 'processor-seezle' %}
                              </li>
                              <li class="inline-flex">
                                {% render 'icon' with 'processor-afterpay' %}
                              </li>
                               {% endcomment %}
                            </ul>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>

