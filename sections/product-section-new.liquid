  assign product = section.settings.product_id
  assign product_form_id = 'product-form-' | append: section.id
%}
<div id="{{ section.settings.section_id }}" style="background-color:{{ section.settings.backgroundColor }}" class="section-{{ section.id }}-padding productSectionAllhid productSection class{{ section.id }} {% if section.settings.newDesign %}newDesign{% endif %}">


{% if section.settings.heading != blank or section.settings.subheading != blank or  section.settings.announcement != blank %}
  
  <div class="topHeadPart" style="padding-left: 10px;
    padding-right: 10px;">
<h2 class="orderNowHead blackHeading">{{ section.settings.subheading  }}</h2>
<h2 class="orderNowHead mainHeading" style="color:{{ section.settings.headingColor }} !important">{{ section.settings.heading }}</h2>
{{ section.settings.text2 }} 


{% if section.settings.announcement != blank %}
<div class="announcementAB">
  {{ section.settings.announcement    }}
</div>
