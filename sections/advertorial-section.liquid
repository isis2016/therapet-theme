<style>
  .wrapperMaindiv {
    width: 100%;
    padding: 20px 0;
    display: flex;
    justify-content: space-between;
    margin-bottom: 50px;
  }
  .leftSideMainWrap {
    width: 73%;
  }
  .rightSideMainWrap {
    width: 25%;
  }
  p.breadC {
    font-size: 12px;
    margin: 20px 0 0;
  }
  .center {
    text-align: center !important
  }
  .imageWithText {
    margin-bottom: 50px;
  }
  p.textLeftHeading.fixSpacesSub {
    margin: 10px 0 20px;
  }
  .mainMotaBlock {
    margin-top: 20px;
    padding: 20px;
    border: 2px solid;
    font-weight: 400;
    text-align: left;
    margin-bottom: 25px;
  }
  a.buttonLast {
    width: 100%;
    max-width: 281px;
    padding: 10px 25px;
    border-style: solid;
    border-width: 1px;
    border-radius: 3px;
    box-shadow: inset 1px 1px 0 0 hsla(0, 0%, 100%, 0.2);
    color: white;
    font-weight: 400;
    display: block;
    margin: auto;
  }
  .stickyClassInside {
    display: flex;
    flex-direction: column;
    flex-wrap: nowrap;
    align-items: center;
    position: sticky;
    top: 40px;
    border-width: 1px;
    border-style: solid;
    border-color: rgb(99, 106, 123);
    padding: 20px;
  }
  .bulletPoint {
    display: flex;
    width: 100%;
    margin: 6px 0;
    align-items: center;
  }
  img.reviewImage {
    width: 75px;
    height: 75px;
    object-fit: cover;
    border-radius: 50%;
    margin: 20px 30px;
    flex-shrink: 0;
  }
  .singleReviewWrap {
    display: flex;
    margin: 20px 0;
    padding: 10px;
    border-radius: 10px;
    border: 2px dashed;
  }
  span.nameRV {
    margin-top: 20px;
    display: block;
  }
  .rightheading {
    font-size: 22px;
    padding: 10px 0;
  }
  .starimage {
    max-width: 100px;
    padding-bottom: 10px;
  }
  .reviewtext {
    font-size: 14px;
  }
  .rightSideheading {
    font-size: 20px;
    padding: 8px 0;
  }
  .Mainimage {
    max-width: 250px;
  }
  p.textLeftHeading {
    font-size: 0.9em;
  }
  .wrapFeaturesSection {
    display: flex;
    margin: 20px 0 50px;
    border: 3px dashed;
    padding: 0 15px 15px;
    justify-content: space-between;
  }
  .rightbutton {
    box-shadow: rgba(37, 42, 50, 0.32) 0 1px 4px 1px;
    border-radius: 50px;
    margin: 10px 10px 20px;
    padding: 15px 30px;
    text-decoration: none;
    color: #fff;
    font-weight: 600;
    font-size: 14px;
  }
  h1.leftSideHeading.h2 {
    margin-bottom: 20px;
  }
  .tickimage {
    max-width: 20px;
    margin-right: 10px;
  }
  .texttexttext {
    font-size: 12px;
  }
  .mediaElementLEFT {
    width: 100%;
    height: auto;
    object-fit: contain;
    border-radius: 10px;
    margin: 15px 0;
  }
  .newBullet {
    font-size: 1em;
    margin: 5px 0;
  }
  .tickimage.image2 {
    margin-right: 15px;
    width: 30px;
    max-width: 100%;
  }
  .wrapFeaturesSection img {
    width: 48%;
    height: fit-content;
    border-radius: 10px;
    object-fit: contain;
  }
  .rightSideWrapF {
    width: 49%;
  }
  .wrapLEFTmediaCap {
    border-radius: 10px;
    margin-top: 35px;
  }
  .wrapLEFTmediaCap .mediaElementLEFT {
    margin-top: -15px;
  }
  .wrapLEFTmediaCap p.subHeadCap {
    padding: 0 10px 15px;
    font-size: 0.9em;
    font-weight: 600;
    margin-top: -10px;
  }
  .pictureReviews {
    margin-bottom: 30px;
    display: block;
  }
  .pictureReview {
    width: 100%;
    display: block;
    margin: 20px auto;
    max-width: 600px;
  }
  .day-step-text {
    font-size: 16px;
    line-height: 20px;
    display: block;
  }
  .day-step {
    width: 60px;
    float: left;
    background-color: var(--dark-green);
    color: var(--white);
    text-align: center;
    margin-bottom: 10px;
    margin-right: 20px;
    padding: 5px 0 5px 2px;
    font-size: 2.5em;
    line-height: 1em;
    position: relative;
    bottom: -5px;
  }

  @media(max-width: 1200px) {
    .leftSideMainWrap {
      width: 65%;
    }
    .rightSideMainWrap {
      width: 33%;
    }
  }

  @media(max-width: 850px) {
    .rightSideMainWrap {
      display: none;
    }
    .leftSideMainWrap {
      width: 100%;
    }
    .wrapFeaturesSection img {
      width: 100%;
    }
    .rightSideWrapF {
      width: 100%;
    }
    .wrapFeaturesSection {
      flex-direction: column;
    }
    img.reviewImage {
      margin: 15px 15px 0 4px;
    }
  }

  /*! CSS Used for Facebook Post */

  .facebook-post {
    max-width: 600px;
    border: 1px solid #ccc;
    border-radius: 10px;
    margin: 30px auto;
    padding: 15px 20px 20px;
    box-shadow: 0 0 6px rgba(0, 0, 0, 0.2);
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
  }
  .dots {
    cursor: pointer;
    position: absolute;
    right: 0;
  }
  .post-meta {
    align-items: center;
    margin-bottom: 10px;
    display: flex;
    position: relative;
  }
  .postime {
    align-items: center;
    display: flex;
  }
  .likes-comments {
    justify-content: space-between;
    display: flex;
  }
  .post-text {
    color: #212121;
    margin-bottom: 10px;
    font-size: 14px;
    line-height: 19px;
  }
  .emojis {
    cursor: pointer;
    display: flex;
  }
  .name {
    color: #3658a6;
    margin-top: 0;
    margin-bottom: 1px;
    font-size: 14px;
    font-weight: 700;
    line-height: 19px;
  }
  .user-picture {
    width: 40px;
    height: 40px;
    border-radius: 100%;
    margin-right: 8px;
  }
  .likers {
    cursor: pointer;
    border-top: 1px solid #ccc;
    border-bottom: 1px solid #ccc;
    justify-content: space-around;
    margin-top: 10px;
    padding: 8px 20px;
    display: flex;
  }
  .numcount {
    color: #a0a0a0;
    cursor: pointer;
    margin-left: 8px;
    font-size: 14px;
    font-weight: 400;
    line-height: 20px;
  }
  .comments-block {
    display: flex;
  }
  .emoji-reaction {
    width: 18px;
    height: 18px;
  }
  .post-time-num {
    color: #65676b;
    margin-right: 8px;
    font-size: 14px;
    font-weight: 400;
    line-height: 20px;
  }
  .post-time-img {
    width: 16px;
    height: 16px;
    opacity: 0.8;
  }
  .post-image {
    width: 100%;
    margin-bottom: 10px;
  }
  @media screen and (max-width: 767px) {
    .hideme {
      display: none;
    }
  }
  @media screen and (max-width: 479px) {
    .likers {
