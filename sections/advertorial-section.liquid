      padding: 0 1.2rem;
    }

    .ratings-meta > * {
      flex: 1 1 50%;
    }

    .stars-text {
      font-size: 14px;
      line-height: 16px;
    }

    .wrapperMaindiv {
      margin-bottom: 0 !important;
    }


    /* Css of Uss */
    .notify_2.border-1 img {
      width: 3rem;
      margin-left: 5px;
    }

    .notify_2.border-1 p {
      color: white;
    }

    .notify_2 {
      border-width: 1px;
      background: #818cf8;
      color: #312e81;
      border: 1px solid #1e1b4b;
      gap: 1rem;
    }

    .w-embed.w-script {
      font-size: 16px;
    }

    a.link-6,
    a.link-7,
    .action-button {
      text-decoration: none;
    }

    .notify_1 {
      background-color: #5d3891;
      color: #fff;
      margin-bottom: 15px;
      padding: 10px;
      display: flex;
      align-items: center;
      gap: 1rem;
    }

    .notify_1 .w-embed.w-script p {
      font-weight: 600;
      color: #FFF;
      font-size: 1.5rem;
      margin: 0;
    }

    .center-text {
      text-align: center;
      margin-top: 18px;
      font-size: 12px;
      line-height: 26px;
    }

    .left-block p,
    .right-block:is(p, li > a) {
      font-size: 13px;
      color: white;
    }

    .right-block:is(li > a:hover) {
      color: #ddd;
    }

    .content-footer {
      display: grid;
      grid-template-columns: 1fr 0.5fr;
      grid-gap: 2rem;
    }

    .container-footer {
      max-width: 1024px;
      margin-inline: auto;
      padding: 3.5rem 0;
    }

    .image-13 {
      width: 250px;
    }

    @media screen and (max-width: 767px) {

      .sections-container {
        grid-template-columns: 1fr !important;
      }

    }
  </style>
{% endif %}

