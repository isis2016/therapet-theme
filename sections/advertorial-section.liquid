<section class="page-width">
  {% if section.settings.breadcrumb != blank %}
    <p class="breadC">{{ section.settings.breadcrumb }}</p>
  {% endif %}
  <div class="wrapperMaindiv">
    {% render 'leftSideMainWrap' %}
    {% render 'rightSideMainWrap' %}
  </div>
</section>

{% if section.settings.checkbox %}
  {% render 'stickybtn' %}
{% endif %}

{% if section.settings.advertorial %}
  <script>
    document.addEventListener("DOMContentLoaded", () => {

      var reviewBlockCheck = document.querySelector('.advertorial-container');
      if (!reviewBlockCheck) {
        mainContent = document.querySelector("main#MainContent");

        advertorialContainer = document.createElement('div');
        advertorialContainer.classList.add('sections-container');

        sectionsContainer = document.createElement('div');
        sectionsContainer.classList.add('advertorial-container');

        sectionsContainer.append(...mainContent.childNodes);

        reviewBlock = document.createElement('div');
        reviewBlock.classList.add('reviews-block');
        reviewBlock.innerHTML = `
          <div class="ratings-titler">About This Article</div>
            <div class="ratings-meta">
              <div class="ratings-col wborder">
                <i class="stamped-fa stamped-fa-star "></i><i class="stamped-fa stamped-fa-star "></i><i class="stamped-fa stamped-fa-star "></i><i class="stamped-fa stamped-fa-star "></i><i class="stamped-fa stamped-fa-star-half-o "></i>
                <div class="stars-text">4.62 Based on 1,161 reviews</div>
              </div>
              <div class="ratings-col">
                <div class="stars-text">Views: <strong>114,479</strong></div>
              </div>
              <img src="https://about.bugmd.com/co/lp/f&amp;t-A3-LD/images/arrow-left_1arrow-left.png" loading="lazy" alt="" class="arrow">
            </div>
          </div>
        `;

      advertorialContainer.appendChild(sectionsContainer);
      advertorialContainer.appendChild(reviewBlock);
      
      mainContent.appendChild(advertorialContainer);
      mainContent.appendChild(document.querySelector(".container-footer").parentNode.parentNode);
      mainContent.insertBefore(document.querySelector(".topBARS").parentNode, mainContent.firstChild);

      }
    });
  </script>

  <style>
    .shopify-section:has( > .topBARS) {
      width: 100%;
    }

    .shopify-section:has(.container-footer) {
      width: 100%;
