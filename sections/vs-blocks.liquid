<style>
.BLOCKMainCLASS {
    display: flex;
    justify-content: space-evenly;
    flex-wrap: nowrap;
    padding: 25px 0px;
  padding-bottom: 0px;
    position: relative;
    align-items: flex-start;
}

.UpperMainText {
    text-align: center;
    width: 100%;
    margin: auto;
    padding: 0 20px;    max-width: 1111px;
}
.UpperMainText h2{
    padding-top: 50px;
    font-weight: 900; 
    margin: 0px;
}

.BLOCKLOGOS {
    width: 44%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.BLOCKIMAGE {
    width: 100%;
    object-fit: contain;
    margin: 0px 0px 30px 0px;
}

.BLOCKTEXT {
    width: 100%;
    font-weight: 700;
    font-size: 22px;
    margin: 0px;
    text-align: left !important;
}
.BlockrichText {
    font-size: 15px;
}

.Circalbg {
    display: flex;
    width: 80px;
    height: 80px;
    border-radius: 50%;
    font-size: 25px;
    color: #070808;
    position: absolute;
    left: 50%;
    top: 25%;
    transform: translateX(-50%);
    align-items: center;
    justify-content: center;
}

.htmlContent {
    width: 100%;
    margin-bottom: 30px;
}
.htmlContent ul{
    list-style: none;
    padding-left: 0px;
}
.htmlContent li{
    font-size: 16px;
    display: flex;
    justify-content: flex-start;
    align-items: center;
margin-bottom: 10px;
}
.htmlContent svg {
    margin-right: 12px;
    flex-shrink: 0;
}

@media (max-width:1000px) {
.Circalbg {
    top: 20%;
}
}

@media (max-width:800px) {
.BLOCKMainCLASS {
    flex-direction: column;
}
.BLOCKLOGOS {
    width: 100%;
}
.BlockrichText {

}
.Circalbg {
    width: 60px;
    height: 60px;
    top: 46%;
}
.htmlContent {
    margin-bottom: 60px;
}.Circalbg {
    position: relative;
    top: auto;
    margin: -30px 0 30px;
}
}
</style>
  
<section class="SabSaPala" style="background-color: {{section.settings.BlockColor}}">

<div class="">
<div class="UpperMainText">
<h2>{{ section.settings.Text }}</h2>
<p class="">{{ section.settings.richText }}</p>
</div>

<div class="page-width BLOCKMainCLASS">
{% for block in section.blocks %}
<div class="BLOCKLOGOS">

{% if block.settings.videoOpt != blank %}
  <video src="{{ block.settings.videoOpt }}" 
         type="video/mp4" class="BLOCKIMAGE"
         loop muted playsinline autoplay disablepictureinpicture controlslist="nodownload" height="auto"
         style="margin:auto;width:100%;">
  </video>
{% else %}
<img class="BLOCKIMAGE lazyload"
  src="{{ block.settings.BlockImage | img_url: 'master'}}"
  loading="lazy"  width="100%" height="100%"
  alt="{{ block.settings.BlockImage.alt }}">
{% endif %}
<h3 class="BLOCKTEXT">{{ block.settings.BlockText }}</h3>
<span class="BlockrichText">{{ block.settings.BlockrichText }}</span>

<div class="htmlContent">
{{ block.settings.content | replace: '[Tick]','<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24" height="24" viewBox="0 0 24 24" fill="none"><circle cx="12" cy="12" r="12" fill="#40C234"/><rect x="2" y="2" width="20" height="20" fill="url(#pattern1)"/><defs><pattern id="pattern1" patternContentUnits="objectBoundingBox" width="1" height="1"><use xlink:href="#image0_60_1106" transform="scale(0.002 0.00222222)"/></pattern><image id="image0_60_1106" width="500" height="500" xlink:href="data:image/png;base64,"/></defs></svg>' | replace: '[Cross]', '<svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewBox="0 0 14 14" fill="none"><path d="M1.4 14L0 12.6L5.6 7L0 1.4L1.4 0L7 5.6L12.6 0L14 1.4L8.4 7L14 12.6L12.6 14L7 8.4L1.4 14Z" fill="#EE3624"/></svg>' }}
</div>
</div>

  {% if forloop.first %}
{% if section.settings.vscheckbox %}
<div class="Circalbg" style="background-color: {{ section.settings.vscolor }}"><span class="circleTEXT">{{ section.settings.vstext }}</span></div>
{% endif %}
    {% endif %}
{% endfor %}


</div>

</div>

</section>

{% schema %}
  {
    "name": "VS Block",
    "settings": [
    {
      "type": "color",
      "id": "BlockColor",
      "label": "Background Color"
    },
    {
      "type": "text",
      "id": "Text",
      "label": "Heading"
    },
    {
      "type": "richtext",
      "id": "richText",
      "label": "Text"
    },
    {
      "type": "checkbox",
      "id": "vscheckbox",
      "label": "Add VS Checbox",
      "default": true
    },
    {
      "type": "color",
      "id": "vscolor",
      "label": "VS Background"
    },
    {
      "type": "text",
      "id": "vstext",
      "label": "VS Text"
    }
    ],
    "blocks": [
      {
        "type": "Blocks",
        "name": "Blocks",
        "limit": 2,
        "settings":[
          {
            "type": "text",
            "id": "videoOpt",
            "label": "Add mp4 video"
          },
          {
            "type": "image_picker",
            "id": "BlockImage",
            "label": "Image"
          },
          {
            "type": "text",
            "id": "BlockText",
            "label": "Heading"
          },
          {
            "type": "richtext",
            "id": "BlockrichText",
            "label": "Text"
          },
          {
            "type":"html",
            "id":"content",
            "label":"Content"
          }
        ]
      }
    ],
    "presets": [{"name": "VS Block"}]
  }
{% endschema %}