function initializePopup(showButtonSelector, closeButtonId, containerId) {
    var showPopupButton = document.querySelector(showButtonSelector);
    var closePopupButton = document.getElementById(closeButtonId);
    var popupContainer = document.getElementById(containerId);

    showPopupButton.addEventListener('click', function () {
        if (popupContainer.style.display !== 'block') {
            popupContainer.style.display = 'block';
            popupContainer.style.transform = 'translateY(0)';
        } else {
            closePopup();
        }
    });

    closePopupButton.addEventListener('click', closePopup);
  

    function closePopup() {
        popupContainer.style.transform = 'translateY(100%)';
        setTimeout(function () {
            popupContainer.style.display = 'none';
        }, 300);
    }
}
  
