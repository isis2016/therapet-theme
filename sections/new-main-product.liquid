$(window).scroll(function() {
    var scrollTrigger1 = 1200; // Set the first scroll trigger to 800 pixels
    var scrollTrigger2 = 2300; // Set the second scroll trigger to 1200 pixels

    var $popBtn = $('.pop-btn');

    if ($(this).scrollTop() > scrollTrigger1 && $(this).scrollTop() <= scrollTrigger2) {
        // Add the 'newClass' when scrolling beyond 800 pixels and within 1200 pixels
        $popBtn.addClass('newClass');
    } else {
        // Remove the 'newClass' when scrolling back above 800 pixels or beyond 1200 pixels
        $popBtn.removeClass('newClass');
    }
});
  
  function toggleCheckbox(label) {
